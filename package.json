{
    "name": "express_mongo_db",
    "version": "1.0.0",
    "description": "Express mongodb APIs",
    "author": "Hoang",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/hoangnguyen1247/express-mongo-quickstart.git",
        "directory": "express-mongo-quickstart"
    },
    "devDependencies": {
        "@types/express": "^4.17.1",
        "@types/node": "^10.12.0",
        "cross-env": "^6.0.0",
        "nodemon": "^1.19.2",
        "ts-node": "^3.3.0"
    },
    "dependencies": {
        "@google-cloud/logging-bunyan": "^0.9.4",
        "axios": "^0.18.0",
        "bcryptjs": "^2.4.3",
        "bluebird": "^3.5.1",
        "body-parser": "^1.18.1",
        "bunyan": "^1.8.12",
        "cookie-parser": "^1.4.3",
        "express": "^4.15.4",
        "express-status-monitor": "^1.2.3",
        "http-errors": "^1.7.1",
        "jsonwebtoken": "^8.2.0",
        "kafka-node": "^3.0.1",
        "mongodb": "^3.1.13",
        "mongoose": "^5.7.5",
        "morgan": "^1.9.0",
        "multer": "^1.3.0",
        "node-dependency-injection": "^2.2.1",
        "object-assign": "^4.1.1",
        "redis": "^2.8.0",
        "reflect-metadata": "^0.1.10",
        "request-promise": "^4.2.2",
        "rotating-file-stream": "1.3.7",
        "serve-favicon": "^2.5.0",
        "slug": "^1.0.0",
        "string-template": "^1.0.0",
        "typeorm": "^0.2.14",
        "typescript": "^3.1.3"
    },
    "scripts": {
        "start": "cross-env NODE_ENV=local nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec node --inspect=127.0.0.1:9236 -r ts-node/register src/index.ts",
        "start:local": "NODE_ENV=local ts-node src/index.ts",
        "start:dev": "NODE_ENV=development ts-node src/index.ts",
        "start:stag": "NODE_ENV=staging ts-node src/index.ts",
        "start:prod": "NODE_ENV=production ts-node src/index.ts",
        "start:deploy": "npm run ts-node -- src/index.ts",
        "start:docker": "npm run ts-node -- src/index.ts",
        "compile": "tsc",
        "pm2:dev": "pm2 deploy development",
        "pm2:prod": "pm2 deploy production",
        "ts-node": "./node_modules/.bin/ts-node",
        "typeorm": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm --config ./src/ormconfig"
    }
}
